        "jsdoc": "^3.4.2",
        "docdash": "^1.2.0"
    },
    "bin" : {
        "cafjs" : "./bin/caf.js"
    },
    "scripts": {
        "eslint" : "./node_modules/.bin/eslint -c .eslintrc.js .",
        "eslintfix" : "./node_modules/.bin/eslint -c .eslintrc.js --fix .",
