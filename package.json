    "author": "Antonio Lain <antlai@cafjs.com>",
    "license": "Apache-2.0",
    "dependencies": {
        "minimist" : "^1.2.0",
        "glob" : "^7.1.4",
        "mustache" : "^3.1.0",
        "caf_core": "^0.4.0",
        "caf_srp": "^0.4.0",
        "mkdirp": "^0.5.1",
        "tar" : "^4.2.0",
        "rimraf" : "^2.5.0",
        "rsync" : "^0.6.1",
        "fstream" : "^1.0.8",
        "dockerode": "^2.5.3",
        "find-yarn-workspace-root" : "^2.0.0"
    },
    "devDependencies": {
        "eslint": "^6.8.0",
